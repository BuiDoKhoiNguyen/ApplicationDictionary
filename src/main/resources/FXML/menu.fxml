<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="570.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.MenuController">
   <children>
      <AnchorPane prefHeight="570.0" prefWidth="55.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="570.0" prefWidth="55.0" styleClass="vbox" stylesheets="@../css/menu.css">
               <children>
                  <ImageView cache="true" fitHeight="64.0" fitWidth="69.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/dictionary.png" />
                     </image>
                     <VBox.margin>
                        <Insets bottom="30.0" top="30.0" />
                     </VBox.margin>
                  </ImageView>
                  <Button id="search" fx:id="searchButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#searchFunction" prefHeight="60.0" prefWidth="145.0" stylesheets="@../css/menu.css" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button id="translate" fx:id="translateButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#translateFunction" prefHeight="60.0" prefWidth="145.0" stylesheets="@../css/menu.css" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button id="favourite" fx:id="favouriteButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#favouriteFunction" prefHeight="60.0" prefWidth="145.0" stylesheets="@../css/menu.css" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button id="editWord" fx:id="saveButton" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="60.0" prefWidth="145.0" stylesheets="@../css/menu.css" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button id="playGame" fx:id="gameButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="145.0" />
                  <Button mnemonicParsing="false" prefHeight="86.0" prefWidth="66.0" style="-fx-background-color: Transparent;" />
                  <Button id="logout" fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutButtonOnAction" prefHeight="60.0" prefWidth="145.0" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="mainAP" layoutX="64.0" prefHeight="570.0" prefWidth="735.0" />
   </children>
</AnchorPane>
